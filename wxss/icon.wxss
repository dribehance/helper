/*icon css*/
.icon {
  display: inline-block;
  width: 30rpx;
  height: 30rpx;
  position: relative;
  top: -2rpx;
  vertical-align: middle;
  text-align: center;
}
.icon.icon-big {
  width: 50rpx;
  height: 50rpx;
}
.icon.icon-category {
  width: 100rpx;
  height: 100rpx;
}
.icon.icon-blank {
  width: 100%;
  height: 300rpx;
}
.icon.icon-error {
  width: 100%;
  height: 750rpx;
}