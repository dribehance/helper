@import "/wxss/common/flexbox.wxss";
@import "/wxss/common/grid.wxss";
@import "/wxss/common/justified.wxss";
@import "/wxss/common/list-group.wxss";
@import "/wxss/common/text.wxss";
@import "/wxss/common/bg.wxss";
@import "/wxss/common/form.wxss";
/*layout css*/
page {
  display: flex;
  flex-direction: column;
  height: 100%;
  background-color: #f8f8f8;
  /*background-color:white;*/
}
navigator {
  box-sizing: border-box;
}
view {
  font-size: 30rpx;
  line-height: 1.5;
  vertical-align: middle;
  box-sizing: border-box;
  color: #2f2f2f;
}
button::after {
  border:none;
}
.scroll-view {
  position: relative;
}
.scroll-content {
  position: absolute;
  left: 0;
  right: 0;
  bottom: 0;
  top: 0;
}
/*float layout*/
.pull-right {
  float:right;
}
.pull-left {
  float:left;
}
.full-width {
  width:100%;
}
.full-height {
  height:100%;
}
.inline {
  display: inline-block;
}
.block {
  display: block;
}
/*section*/
.section {
  padding: 24rpx;
}
.margin-b12 {
  margin-bottom: 12rpx;
}