/*layout css*/
page {
  background-color: #f8f8f8;
  /*background-color:white;*/
  /*height: 100%;*/
}
navigator {
  box-sizing: border-box;
}
view {
  font-size: 30rpx;
  line-height: 1.5;
  vertical-align: middle;
  box-sizing: border-box;
}
/*float layout*/
.pull-right {
  float:right;
}
.pull-left {
  float:left;
}
.full-width {
  width:100%;
}
.full-height {
  height:100%;
}
.inline {
  display: inline-block;
}
.block {
  display: block;
}
/*section*/
.section {
  padding: 24rpx;
}
.margin-b12 {
  margin-bottom: 12rpx;
}